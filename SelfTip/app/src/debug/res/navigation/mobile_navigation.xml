<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_user">

    <fragment
        android:id="@+id/navigation_data"
        android:name="com.edu.nuc.selftip.Fragment.DataFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_data" />

    <fragment
        android:id="@+id/navigation_Maps"
        android:name="com.edu.nuc.selftip.Fragment.MapsFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_maps" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.edu.nuc.selftip.Fragment.UserFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_user" />
</navigation>